{
  "extends": "airbnb-base",
  "env": {
    "browser": false,
    "jest": true,
    "node": true
  },
  "rules": {
    "max-len": ["error", 120],
    "object-curly-newline": ["error", { "consistent": true }],
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "never"
    }],
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-return-await": 0
  }
}
